{ "app": {
   
   "title": "Memory App",
   "version": "1.0.0",
   "supplier": "Open STEMware",
   "copyright": "bitValence",
   "url": "https://github.com/cfs-apps/proxy_mm",

   "description": [
      "This is Basecamp proxy for NASA's Memory Manager App. This repo augments",
      "the MM App repo so MM can be integrated with Basecamp. The 'cfs' and",
      "'requires' JSON objects are populated in mm.json"
   ],

   "proxy": {
      "url": "https://github.com/nasa/mm",
      "branch-tag": "equuleus-rc1",
      "update-files": [
         "mm.json              >> mm.json",
         "CMakeLists.txt       >> CMakeLists.txt",
         "eds/mm.xml           >> eds/mm.xml",
         "fsw/inc/mm_msgids.h  >> fsw/inc/mm_msgids.h"
      ]
   },
      
   "cfs": {
      "cfe-type": "CFE_PROXY"
   },
   
   "requires": []
   
}}

